<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.base.modules.jeebms.mapper.BmsCostRuleIMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  bms_cost_rule_i 
		WHERE
			 cont_no = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="com.base.modules.jeebms.entity.BmsCostRuleI">
		SELECT * 
		FROM  bms_cost_rule_i
		WHERE
			 cont_no = #{mainId} 	</select>
</mapper>
